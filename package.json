{
  "name": "shaco-examples",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "browserify source/index.js -d -v | uglifyjs -c > ./assets/bundle.js",
    "watch": "watchify source/index.js -o ./assets/bundle.js -v",
    "start": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "plugins": [
            [
              "transform-jsx",
              {
                "module": "jsx-to-shaco"
              }
            ]
          ],
          "presets": [
            "es2015",
            "stage-2"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "gzippo": "^0.2.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.1"
  },
  "devDependencies": {
    "babel": "^6.5.0",
    "babel-plugin-transform-jsx": "^1.2.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "jsx-to-shaco": "0.0.3",
    "redux": "^3.3.1",
    "shaco-router": "0.0.2",
    "shadow-component": "^0.2.3",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
